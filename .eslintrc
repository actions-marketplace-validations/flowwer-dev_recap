{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": [
    "dist/**"
  ],
  "globals": {
    "Promise": "readonly"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
  }
}
